package garage;

public class asdfsd {
	public static void main(String[] args) {

		int a = 100; // room size
		int b = 21; // -r9
		int c = 30;
		int leastWaste = c - (a % c);
		System.out.println(leastWaste);

		if (a % c != 0) {
			int cheapest = (a / c + 1) * b;
			System.out.println(cheapest);
		} else {
			int cheapest = (a / c) * b;
			System.out.println(cheapest);
		}

	}

}
